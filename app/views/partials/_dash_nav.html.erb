
    <!-- Header -->
      <header id="header">
        <div class="inner">

          <!-- Logo -->
            <a href="<%= dash_path %>" class="logo">
              <span class="symbol"><img src="/images/logo.svg" alt="" /></span><span class="title">Panther</span>
            </a>

          <!-- Nav -->
            <nav>
              <ul>
                <li><a href="#menu">Menu</a></li>
              </ul>
            </nav>

        </div>
      </header>

    <!-- Menu -->
      <nav id="menu">
        <h2>Menu</h2>
        <ul><% if current_user && current_user.level == "1" %>
          <li><a href="<%= dash_path %>">Home</a></li>
          <li><a href="<%= root_path %>">Visit Your Site</a></li>
          <li><a href="<%= manage_path %>">Post Management</a></li>
          <li><a href="<%= settings_path %>">Site Settings</a></li>
          <li><a href="<%= account_path %>">Your Account</a></li>
          <% if session[:impersonate] %>
          <li><a href="<%= stopimpersonate_path(session[:impersonate]) %>">Stop Impersonation</a>
            <% end %>
          <% else %>
          <li><a href="<%= root_path %>">Home</a></li>
          <li><a href="<%= account_path %>">Your Account</a></li>
          <% end %>
        </ul>
      </nav>
